{
  "name": "<%= appname %>",
  "version": "<%= appversion %>",
  "description": "<%= appdescription %>",
  "main": "",
  "scripts": {
    "test": "eslint . && mocha --reporter spec --timeout 3000" ,
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint"
  },
  "engines": {
    "node": ">=6"
  },
  "keywords": [
    "startup",
    "generator",
    "javascript",
    "karma",
    "vuejs"
  ],
  "author": "<%= appauthor %>",
  "license": "<%= applicense %>",
  "dependencies": {
    "core-js": "^3.4.4",
    "vue": "^2.6.10",    
    "ejs-lint" : "~1.0.1" 
  },
  "devDependencies": {
    "babel-eslint" : "^10.0.3",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "eslint-loader":"3.0.3",
    "eslint-config-prettier": "^4.3.0",  
    "@vue/eslint-config-prettier": "^4.0.1",
    "@vue/cli-service": "^4.1.0",
    "@vue/cli-plugin-babel": "^4.1.0",
    "@vue/cli-plugin-router": "^4.1.0",
    "@vue/cli-plugin-unit-jest": "^4.1.0",
    "@vue/cli-plugin-vuex": "^4.1.0",
    "vue-template-compiler": "^2.6.10" ,
    "mocha": "^6.1.4",
    "prettier": "^1.19.1",
    "css-loader": "^3.4.2",
    "node-sass" : "~4.13.1",
    "sass-loader": "~8.0.2" ,
    "style-loader": "^1.1.3",
    "vue-loader": "^15.8.3",
    "webpack": "^4.41.5",
    "webpack-dev-server": "^3.10.2"
  },
  "files": [
    "app"
  ],
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "trailingComma": "none"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/prettier",
      "@vue/typescript"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "@typescript-eslint/parser"
    }
  }
}
